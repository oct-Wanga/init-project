{
  "name": "Project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate-icon-index": "node ./scripts/generate-icon-index.mjs",
    "generate-icon-types": "node ./scripts/generate-icon-types.mjs",
    "generate-dev-sprite": "npm run generate-icon-index && npm run generate-icon-types && node ./scripts/generate-dev-sprite.mjs",
    "watch-icons": "node ./scripts/watch-icons.mjs",
    "copy-sprite": "node ./scripts/copy-sprite.mjs",
    "dev": "npm run generate-dev-sprite && concurrently \"npm run watch-icons\" \"env-cmd -f .env.local next dev\"",
    "build": "npm run generate-dev-sprite && next build && npm run copy-sprite",
    "start": "next start",
    "build:dev": "env-cmd -f .env.develop next build",
    "start:dev": "env-cmd -f .env.develop next start",
    "build:stg": "env-cmd -f .env.stage next build",
    "start:stg": "env-cmd -f .env.stage next start",
    "build:prod": "env-cmd -f .env.production next build",
    "start:prod": "env-cmd -f .env.production next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.51.21",
    "@tanstack/react-query-devtools": "^5.32.0",
    "@tippyjs/react": "^4.2.6",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dayjs": "^1.11.12",
    "env-cmd": "^10.1.0",
    "framer-motion": "^12.6.2",
    "lottie-react": "^2.4.1",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "next-svgr": "^0.0.2",
    "react": "^18",
    "react-cookie": "^7.2.0",
    "react-dom": "^18",
    "react-loading-skeleton": "^3.4.0",
    "terser-webpack-plugin": "^5.3.14",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "chokidar": "^4.0.3",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "prettier": "^3.2.5",
    "raw-loader": "^4.0.2",
    "stylelint": "^16.16.0",
    "stylelint-config-standard": "^37.0.0",
    "svg-sprite-loader": "^6.0.11",
    "typescript": "^5"
  }
}
